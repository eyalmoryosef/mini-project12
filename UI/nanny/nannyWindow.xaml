<Window x:Class="UI.nannyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI"
        mc:Ignorable="d"
        Title="nannyWindow" Height="450" Width="600
        ">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="70*"/>
        </Grid.RowDefinitions>
        <Button x:Name="add" Content="add nanny" HorizontalAlignment="Left" Height="69" Margin="10,27,0,0" VerticalAlignment="Top" Width="152" Click="add_Click"/>
        <Button x:Name="search" Content="search nanny" HorizontalAlignment="Left" Height="69" Margin="394,27,0,0" VerticalAlignment="Top" Width="152"/>
        <Button x:Name="remove" Content="remove nanny" HorizontalAlignment="Left" Height="69" Margin="198,27,0,0" VerticalAlignment="Top" Width="152"/>
        <Button x:Name="update" Content="update nanny" HorizontalAlignment="Left" Height="69" Margin="10,10,0,0" VerticalAlignment="Top" Width="152" Grid.Row="1"/>
    </Grid>
</Window>
